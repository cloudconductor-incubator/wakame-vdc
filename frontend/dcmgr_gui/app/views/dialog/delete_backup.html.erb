<div id="delete_backup_dialog">
  <div class="message">
    <p><%= t("dialog.delete_backup.message") %></p>
  </div>
  <table id="backups">
    <% @backup_objects.each do |b| %>
    <tr>
      <td><%= t("dialog.delete_backup.backup_id") %></td>
      <td class="backup_id"><%= b["id"] %></td>
    </tr>
    <tr>
      <td class="border"><%= t("dialog.delete_backup.display_name") %></td>
      <td class="border"><%= b["display_name"] %></td>
    </tr>
    <% end %>
  </table>
</div>
